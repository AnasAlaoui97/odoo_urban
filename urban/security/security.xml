<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.module.category" id="module_category_urban">
        <field name="name">URBAN Management</field>
        <field name="description">Urban groups</field>
    </record>

    <record id="group_urban_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="module_category_urban"/>
        <field name="implied_ids" eval="[(4, ref('group_urban_directeur'))]"/>
    </record>
    <record id="group_urban_directeur" model="res.groups">
        <field name="name">Directeur</field>
        <field name="category_id" ref="module_category_urban"/>
        <field name="implied_ids" eval="[(4, ref('group_urban_manager'))]"/>
    </record>
    <record id="group_urban_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="module_category_urban"/>
        <field name="implied_ids" eval="[(4, ref('group_urban_user'))]"/>
    </record>
    <record id="group_urban_user" model="res.groups">
        <field name="name">Utilisateur</field>
        <field name="category_id" ref="module_category_urban"/>
    </record>

</odoo>
